{
  "env": {
    "NODE_ENV": "development"
  },
  "execMap": {
    "js": "node --harmony",
    "py": "python",
    "rb": "ruby"
  },
  "ext": "js json",
  "ignore": [
    "/.tmp/",
    "/app/",
    "/configuration/",
    "/docker/",
    "/gulp_tasks/",
    "/logs/",
    "/node_modules/",
    "/bower_components/",
    "/reference/",
    "/test/",
    "/.*",
    "/*.*"

  ],
  "restartable": "rs",
  "verbose": true
}
